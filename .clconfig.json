{
  "config_version": 1,
  "categories": [
    "ci",
    "docker"
  ],
  "change_types": [
    {
      "short": "feat",
      "long": "Features"
    },
    {
      "short": "imp",
      "long": "Improvements"
    },
    {
      "short": "fix",
      "long": "Bug Fixes"
    }
  ],
  "commit_message": "add changelog entry",
  "changelog_path": "CHANGELOG.md",
  "changelog_dir": null,
  "expected_spellings": {},
  "legacy_version": null,
  "mode": "single",
  "target_repo": "https://github.com/MalteHerrmann/changelog-lint-action",
  "use_categories": true
}
